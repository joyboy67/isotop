!-- colors --!                                                                  
*.cursorColor:  #7c7c7c
*background:	#000
*foreground:	#eeeeee
! black
*color0:		#000000
*color8:		#7c7c7c
! red
*color1:		#ff6c60
*color9:		#ffb6b0
! green
*color2:		#a8ff60
*color10:		#ceffab
! yellow
*color3:		#ffffb6
*color11:		#ffffcb
! blue
*color4:		#6a96ca
*color12:		#b6dcfe
! purple
*color5:		#ff73fd
*color13:		#ff9cfe
! cyan
*color6:		#c6c5fe
*color14:		#dfdffe
! grey
*color7:		#eeeeee
*color15:		#ffffff

xterm*colorUL:          #8ae234
xterm*underlineColor:   #92659a

! Xterm config
xterm*faceName: Hack:pixelsize=12:antialias=true
XTerm.vt100.bellIsUrgent: true
XTerm.vt100.translations: #override \n\
Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
Ctrl Shift <Key>V: insert-selection(CLIPBOARD)
XTerm*loginShell:true
XTerm*locale: utf8
XTerm*renderFont: true
XTerm*saveLines: 4242
XTerm*internalBorder:		12
XTerm*scrollBar:		false
XTerm*selectToClipboard:	true
XTerm*utf8: 1

! ===== xlock
! plain white-on-black lock screen
xlock.description : off
xlock.echokeys    : off
xlock.info        :
xlock.mode        : blank
xlock.username    : "?: "
xlock.password    : "pass: "

! === xclock
xclock*highlight: #ffffff
xclock*hands: #eeeeee


! xmessage
Xmessage.form.okay.shapeStyle:         rectangle
Xmessage*buttons:                       OK
Xmessage*defaultButton:                 OK
Xmessage.form.message.Scroll:          WhenNeeded
Xmessage*borderWidth:                  0
Xmessage*Text.borderWidth:             0
Xmessage*Command.shapeStyle:           rectangle
Xmessage*Command.highlightThickness:    3
Xmessage*Command.internalWidth:         5
Xmessage*Command.internalHeight:        3
Xmessage*Command.borderColor:            #75a9e8
Xmessage*font:-misc-fixed-bold-r-normal-*-18-*-*-*-*-*-iso10646-1


! all
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

*font:-misc-fixed-medium-r-normal-*-14-*-*-*-*-*-iso10646-1

